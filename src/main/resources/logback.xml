<configuration>
    　　　<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    　　　　　 <encoder>
    　　　　　　　　　<pattern>%d{yyyy-MM-dd} [ %thread ] - [ %-5level ] [ %logger{50} : %line ] - %msg%n</pattern>
    　　　　　 </encoder>
    　　　</appender>

    　　　<root level="TRACE">
    　　　　　　<appender-ref ref="STDOUT" />
    　　　</root>
</configuration>